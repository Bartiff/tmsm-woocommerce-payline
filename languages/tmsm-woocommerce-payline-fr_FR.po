msgid "Default language"
msgstr "Langue par défaut"

#: class-wc-gateway-payline.php:677
msgid "Based on browser"
msgstr "Sur la base de navigateur"

#: class-wc-gateway-payline.php:682
msgid "Language used to display Payline web payment pages"
msgstr "Langue utilisée pour afficher les pages de paiement Payline web"

#: class-wc-gateway-payline.php:685
msgid "Payment action"
msgstr "Action de paiement"

#: class-wc-gateway-payline.php:689
msgid "Authorization"
msgstr "Autorisation (validation nécessaire sous 7 jours)"

#: class-wc-gateway-payline.php:690
msgid "Authorization + Capture"
msgstr "Autorisation + Capture (paiement immédiat)"

#: class-wc-gateway-payline.php:692
msgid "Type of transaction created after a payment"
msgstr "Type de transaction créée après un paiement"

#: class-wc-gateway-payline.php:695
msgid "Custom page code"
msgstr "Page personnalisée"

#: class-wc-gateway-payline.php:697
msgid ""
"Code of payment page customization created in Payline Administration Center"
msgstr ""
"Code de la page personnalisée de paiement, créée dans le centre "
"d'administration de  Payline"

#: class-wc-gateway-payline.php:702
msgid "Contract number that determines the point of sale used in Payline"
msgstr "Numéro de contrat qui détermine le point de vente utilisé dans Payline"

#: class-wc-gateway-payline.php:705
msgid "Primary contracts"
msgstr "Contrats primaires"

#: class-wc-gateway-payline.php:707
msgid ""
"Contracts displayed on web payment page - step 1. Values must be separated "
"by ;"
msgstr ""
"Contrats affichés sur la page de paiement de Web - étape 1. Les valeurs "
"doivent être séparées par ;"

#: class-wc-gateway-payline.php:710
msgid "Secondary contracts"
msgstr "Contrats secondaires"

#: class-wc-gateway-payline.php:712
msgid "Contracts displayed for payment retry. Values must be separated by ;"
msgstr ""
"Les contrats affichés pour le paiement \"Réessayer\". Les valeurs doivent "
"être séparées par ;"

#: class-wc-gateway-payline.php:863
#, php-format
msgid "Can't redirect to payment page (error code %s: %s)"
msgstr ""
"Impossible de rediriger vers la page de paiement (code d’erreur %s : %s)"

#: class-wc-gateway-payline.php:866
#, php-format
msgid ""
"You can't be redirected to payment page (error code %s: %s). Please contact "
"us."
msgstr ""
"Vous ne pouvez pas être redirigé vers la page de paiement (code d'erreur "
"%s : %s). Veuillez nous contacter."

#: class-wc-gateway-payline.php:912
#, php-format
msgid "Token %s does not match expected %s for order %s, updating order anyway"
msgstr ""
"Le jeton %s ne correspond pas au jeton attendu %s pour la commande %s, mise "
"à jour de la commande malgré tout"

#: class-wc-gateway-payline.php:920
msgid "Payment successful"
msgstr "Paiement réussi"

#: class-wc-gateway-payline.php:925
msgid "Fraud alert. See details in Payline administration center."
msgstr ""
"Fraude suspectée. Voir les détails dans le centre d’administration Payline."

#: class-wc-gateway-payline.php:929
msgid "Buyer cancelled his payment"
msgstr "L’acheteur a annulé son paiement"

#: class-wc-gateway-payline.php:934
msgid "Payment session expired without transaction"
msgstr "La session de paiement a expiré sans transaction"

#: class-wc-gateway-payline.php:940
msgid "Payment session expired with no redirection on payment page"
msgstr ""
"La session de paiement a expiré avec aucune redirection sur la page de "
"paiement"

#: class-wc-gateway-payline.php:945
msgid "Payment in progress"
msgstr "Paiement en cours"

#: class-wc-gateway-payline.php:951
#, php-format
msgid "Payment refused (code %s): %s"
msgstr "Paiement refusé (code %s) : %s"

